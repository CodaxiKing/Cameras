# Configuração da API
# URL base da sua API backend
EXPO_PUBLIC_API_URL=https://pcountapi.homologa.click/api/v2

# URLs específicas para API V1 e V2
EXPO_PUBLIC_API_URL_V1=https://pcountapi.homologa.click/api/v1
EXPO_PUBLIC_API_URL_V2=https://pcountapi.homologa.click/api/v2

# Configurações de desenvolvimento
# Desabilita dados mock para mostrar erros reais da API
EXPO_PUBLIC_ENABLE_MOCK_FALLBACK=false

# Configurações opcionais
EXPO_PUBLIC_API_TIMEOUT=10000